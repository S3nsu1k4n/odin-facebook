<h1>New Post</h1>

<%= link_to "back", current_user %>

<%= form_with model: @post do |form| %>
  <div class="new-post-form">
    <%= form.label :title %>
    <%= form.text_field :title %>
    <br>
    <%= form.label :content %>
    <%= form.text_area :content %>
    <br>
    <%= form.submit %>

  </div>
<% end %>